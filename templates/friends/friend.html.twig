{% extends 'layout.html.twig' %}
{% block title %}Our Friends - {{ friend.firstname ~ ' ' ~ friend.lastname }}{% endblock %}
{% block main %}
    <p>{{ friend.firstname}} {{ friend.lastname }}</p>
    <p>{{ friend.email }}</p>
    <img src="{{ friend.photo }}" alt="friend_profile">
    <br>
    <a href="{{ url('friends.index') }}">Retour</a>
{% endblock %}